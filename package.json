{
  "name": "seaguntech-expo-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "lint:check": "expo lint",
    "format": "prettier --write \"app/**/*.{js,jsx,ts,tsx}\" \"src/**/*.{js,jsx,ts,tsx}\" \"config/**/*.{js,jsx,ts,tsx}\" \"constants/**/*.{js,jsx,ts,tsx}\" \"types/**/*.{js,jsx,ts,tsx}\" \"__tests__/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"app/**/*.{js,jsx,ts,tsx}\" \"src/**/*.{js,jsx,ts,tsx}\" \"config/**/*.{js,jsx,ts,tsx}\" \"constants/**/*.{js,jsx,ts,tsx}\" \"types/**/*.{js,jsx,ts,tsx}\" \"__tests__/**/*.{js,jsx,ts,tsx}\"",
    "check-types": "tsc --noEmit",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "supabase:migrate": "supabase db push",
    "supabase:migrate:local": "supabase migration up --local",
    "supabase:reset": "supabase db reset",
    "supabase:seed": "tsx supabase/scripts/seed.ts",
    "supabase:setup": "npm run supabase:migrate && npm run supabase:seed",
    "e2e:install": "curl -fsSL \"https://get.maestro.mobile.dev\" | bash",
    "e2e:test": "maestro test e2e/maestro/flows",
    "e2e:test:smoke": "maestro test e2e/maestro/config/test-suites.yaml --suite smoke",
    "e2e:test:ci": "maestro test e2e/maestro/config/test-suites.yaml --suite ci",
    "e2e:test:auth": "maestro test e2e/maestro/flows/auth",
    "e2e:record": "maestro record e2e/maestro/flows"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "^7.2.0",
    "@shopify/flash-list": "^2.0.2",
    "@stripe/stripe-react-native": "^0.50.3",
    "@supabase/supabase-js": "^2.93.2",
    "@tanstack/react-query": "^5.90.20",
    "clsx": "^2.1.1",
    "expo": "~54.0.32",
    "expo-apple-authentication": "^8.0.8",
    "expo-audio": "~1.1.1",
    "expo-auth-session": "^7.0.10",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-device": "^8.0.10",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "^17.0.10",
    "expo-linking": "~8.0.11",
    "expo-localization": "^17.0.8",
    "expo-notifications": "^0.32.16",
    "expo-router": "~6.0.22",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "i18next": "^25.8.0",
    "nativewind": "5.0.0-preview.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.4",
    "react-native": "0.81.5",
    "react-native-css": "3.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^4.1.1",
    "react-native-purchases": "^9.7.2",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "resolutions": {
    "lightningcss": "1.30.1"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-typescript": "^7.28.5",
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.1.0",
    "@types/react": "~19.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "lint-staged": "^16.2.7",
    "patch-package": "^8.0.1",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
